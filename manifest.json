{
	"name": "WeebSkipper",
	"description": "Skip those darn filler episodes (｡•̀ᴗ-)✧",
	"version": "0.1.0",
	"manifest_version": 2,
	"icons": {
		"16": "./logo/fs16.png",
		"32": "./logo/fs32.png",
		"64": "./logo/fs64.png",
		"128": "./logo/fs128.png"
	},
	"background": {
		"scripts": ["./scripts/background.js"]
	},
	"options_page": "./pages/options.html",
	"browser_action": {
		"default_popup": "./pages/popup.html"
	},
	"permissions": [
		"tabs",
		"https://4anime.to/*",
		"https://www.animefillerlist.com/*"
	]
}